#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <ctype.h>
int main() {
    char num[1000];
    fgets(num,1000,stdin);   //input
    
    int l=strlen(num);        //length of string
    
    int a=0;
    int b=0;
    int c=0;
    int d=0;
    int e=0;
    int f=0;
    int g=0;
    int h=0;
    int x=0;
    int j=0;
    
    for(int i=0;i<l;i++)
    {
      
        if(num[i]=='0'){
        ++a;
        continue;
        }
        else if(num[i]=='1'){
        ++b;
        continue;
        }
        else if(num[i]=='2'){
        ++c;
        continue;
        }
        else if(num[i]=='3'){
        ++d;
        continue;
        }
        else if(num[i]=='4'){
        ++e;
        continue;
        }
        else if(num[i]=='5'){
        f++;
        continue;
        }
        else if(num[i]=='6')
        g++;
        else if(num[i]=='7')
        h++;
        else if(num[i]=='8')
        x++;
        else if(num[i]=='9')
        j++;
        else
        continue;
        
       
    }
        printf("%d %d %d %d %d %d %d %d %d %d",a,b,c,d,e,f,g,h,x,j);
    return 0;
}
